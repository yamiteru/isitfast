{
  "version": "0.0.5",
  "name": "isitfast",
  "author": "Miroslav Vršecký <yamiteru@icloud.com>",
  "description": "A modular benchmarking library with V8 warmup and cpu/ram denoising for the most accurate and consistent results.",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.map",
    "dist/**/*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yamiteru/isitfast.git",
    "directory": "src"
  },
  "keywords": [
    "benchmark",
    "performance",
    "stats",
    "measure",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamiteru/isitfast/issues"
  },
  "homepage": "https://github.com/yamiteru/isitfast",
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "test:coverage": "jest --collectCoverage",
    "test": "jest",
    "build": "pnpm clean && ts-node scripts/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "release": "release-it",
    "example:emptyFunctions": "node --expose-gc -r ts-node/register examples/emptyFunctions.ts",
    "example:arrayLoops": "node --expose-gc -r ts-node/register examples/arrayLoops.ts"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "3.1.0",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.41",
    "@swc/helpers": "0.4.14",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.5",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "auto-changelog": "2.4.0",
    "chalk": "4.1.2",
    "esbuild": "0.17.12",
    "esbuild-plugin-swc": "1.0.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "prettier": "2.8.6",
    "release-it": "15.9.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "next"
  },
  "dependencies": {
    "elfs": "0.0.7",
    "ueve": "1.2.2"
  }
}
