{
  "name": "isitfast",
  "type": "module",
  "version": "0.0.8",
  "main": "dist/index.js",
  "bin": {
    "isitfast": "./dist/index.js"
  },
  "scripts": {
    "fix:format": "prettier --write \"**/*.ts\"",
    "fix:lint": "eslint --fix --ext .ts .",
    "fix": "pnpm fix:lint && pnpm fix:format",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:lint": "eslint --ext .ts .",
    "check": "pnpm check:lint && pnpm check:format",
    "build": "rollup --config rollup.config.js && chmod +x dist/index.js && npm install -g .",
    "dev": "pnpm src/index.ts"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "11.1.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.86",
    "@types/node": "20.6.3",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "rollup": "3.29.2",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "2.2.2",
    "ink": "4.4.1",
    "meow": "12.1.1",
    "react": "18.2.0"
  }
}
