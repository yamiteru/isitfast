{
  "version": "0.0.8",
  "name": "isitfast",
  "author": "Miroslav Vršecký <yamiteru@icloud.com>",
  "description": "A modular benchmarking library with V8 warmup and cpu/ram denoising for the most accurate and consistent results.",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.mjs",
    "dist/**/*.map",
    "dist/**/*.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yamiteru/isitfast.git",
    "directory": "src"
  },
  "keywords": [
    "benchmark",
    "performance",
    "stats",
    "measure",
    "javascript",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamiteru/isitfast/issues"
  },
  "homepage": "https://github.com/yamiteru/isitfast",
  "scripts": {
    "test:coverage": "jest --collectCoverage",
    "test": "jest",
    "build": "pnpm clean && ts-node scripts/build.ts && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "example:emptyFunctions": "node --expose-gc -r ts-node/register examples/emptyFunctions.ts",
    "example:test": "node --expose-gc -r ts-node/register examples/test.ts",
    "example:test2": "node --expose-gc -r ts-node/register examples/test2.ts",
    "example:arrayLoops": "node --expose-gc -r ts-node/register examples/arrayLoops.ts"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.41",
    "@swc/helpers": "0.4.14",
    "@types/benchmark": "^2.1.2",
    "@types/node": "18.15.5",
    "chalk": "4.1.2",
    "esbuild": "0.17.12",
    "esbuild-plugin-swc": "1.0.1",
    "ts-node": "10.9.1",
    "typescript": "next"
  },
  "dependencies": {
    "elfs": "0.0.7",
    "ueve": "1.2.2"
  }
}
